using System;

namespace ConsoleApp4
{
    class Program
    {
        static void Main(string[] args)
        {
            int N = Convert.ToInt32(Console.ReadLine());
            string name = Console.ReadLine();
            if ((N % 2)==0)
            {
                for (int i = 0; i < N; i++)
                {
                    Console.WriteLine(name);
                }
            }
            else
            {
                Console.WriteLine("C# is very cool");
            }
        }
    }
}
